#include<iostream>
using namespace std;
int main()
{
    int i,n,sum1=0,sum2=0;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    {
    for(i=0;i<n;i++)
    {
        if(a[i]%2==0)
        {
            sum1++;
        }else
        {
            sum2++;
        }
    }
    cout<<"Odd:"<<sum2<<endl;
    cout<<"Even:"<<sum1;
}
}
