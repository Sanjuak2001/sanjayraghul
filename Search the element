#include<iostream>
using namespace std;
int main()
{
    int n,x,c;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cin>>x;
    for(int i=0;i<n;i++)
    {
        if(x==a[i])
        {
            c++;
        }
    }
    if(c>0)
    {
        cout<<x<<" is present in the array";
    }
    else
    {
        cout<<x<<" is not present in the array";
    }
}
