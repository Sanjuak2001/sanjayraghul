#include<iostream>
using namespace std;
int main()
{
    int n,c1,c2;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        if(a[i]%2==0)
        {
            c1++;
        }
        else
        {
            c2++;
        }
    }
    if(c1==n)
    {
        cout<<"Even";
    }
    else if(c2==n)
    {
        cout<<"odd";
    }
    else
    {
        cout<<"Mixed";
        
    }
}
